    "cordova-android",
    "cordova-browser",
    "UMD",
    "library",
    "electron",
    "NW.js"
  ],
  "author": "Jason Dreyzehner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devjeonghwan/cordova-plugin-qrscanner/issues"
  },
  "homepage": "https://github.com/devjeonghwan/cordova-plugin-qrscanner",
  "main": "dist/cordova-plugin-qrscanner-lib.min.js",
  "dependencies": {
    "qrcode-reader": "^1.0.4",
    "webrtc-adapter": "^3.1.4"
  },
  "devDependencies": {
    "raw-loader": "^0.5.1"
  },
  "config": {
